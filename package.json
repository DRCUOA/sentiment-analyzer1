{
  "name": "sentiment-analyzer1",
  "version": "1.0.0",
  "description": "Learning exercise on building a simple text classifier from scratch using a Naive Bayes classifier algorithm to detect and learn sentiment in text with vanilla JavaScript",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "log:routes": "NODE_ENV=development DEBUG=debug:r* nodemon app.js",
    "log:controllers": "NODE_ENV=development DEBUG=debug:c* nodemon app.js",
    "log:models": "NODE_ENV=development DEBUG=debug:d* nodemon app.js",
    "log:all": "NODE_ENV=development DEBUG=debug:* nodemon app.js",
    "runOnWin": "set NODE_ENV=development && set DEBUG=* && nodemon app.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "start": "NODE_ENV=production node app.js",
    "dev": "NODE_ENV=development nodemon app.js",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "build": "babel src -d dist",
    "lint": "eslint 'src/**'",
    "format": "prettier --write 'src/**'",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "serve-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DRCUOA/sentiment-analyzer1.git"
  },
  "author": "Richard Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DRCUOA/sentiment-analyzer1/issues"
  },
  "homepage": "https://github.com/DRCUOA/sentiment-analyzer1#readme",
  "dependencies": {
    "axios": "^1.3.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "luxon": "^3.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "openai": "^4.11.0",
    "quagga": "^0.6.16",
    "sql-template-strings": "^2.2.2",
    "sqlite3": "^5.1.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
    "chance": "^1.1.11",
    "csv-parser": "^3.0.0",
    "eslint": "^8.5.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.0.1",
    "papaparse": "^5.4.1",
    "prettier": "^3.0.3",
    "sinon": "^16.0.0",
    "supertest": "^6.3.3"
  },
  "overrides": {
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2",
    "got": "11.8.6",
    "semver": "7.5.3"
  }
}
